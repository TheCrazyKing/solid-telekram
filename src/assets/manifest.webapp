{
	"name": "KGram",
	"description": "Telegram Client for KaiOS.",
	"display": "Telegram",
	"short_name": "Telegram",
	"version": "1.0.8",
	"launch_path": "/index.html",
	"theme": "#fff",
	"theme_color": "#fff",
	"background_color": "#fff",
	"orientation": "default",

	"categories": ["social"],

	"icons": {
		"56": "/icon56.png",
		"112": "/icon112.png"
	},
	"developer": {
		"name": "Cyan",
		"url": "https://discord.gg/W9DF2q3Vv2"
	},
	"origin": "app://telekram.cyankindasus.com",
	"type": "certified",
	"permissions": {
		"spatialnavigation-app-manage": {},
		"systemXHR": {},
		"audio-capture": {},
		"video-capture": {
			"description": "Reading Codes using the Camera"
		},
		"desktop-notification": {},
		"tcp-socket": { "description": "Required to do any TCP" },
		"device-storage:sdcard": { "access": "readwrite" },
		"volumemanager": {}
	},
	"dependencies": {
		"ads-sdk": "1.5.8"
	},
	"locales": {
		"en-US": {
			"name": "KGram",
			"subtitle": "Telegram for KaiOS",
			"description": "Telegram Client for KaiOS.",
			"display": "Telegram",
			"short_name": "Telegram"
		}
	},

	"priority": "high",

	"default_locale": "en"
}
